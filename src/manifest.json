{
    "manifest_version": 3,
    "name": "plunger",
    "version": "1.0.2",
    "description": "A Youtube adblock-block-un-blocker browser extension",
    "author": "fraser.chapman@gmail.com",
    "default_locale": "en",
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    }
}