{
    "manifest_version": 3,
    "name": "plunger",
    "version": "0.0.0.11",
    "description": "A Youtube adblock-block-un-blocker browser extension",
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://www.youtube.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "plunger@fraser"
        }
    }
}